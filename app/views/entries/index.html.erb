

<h1>My Rails Girls Diary</h1>
  <div>
    <% for entry in @entries %>
    <h2><%= link_to(entry.title, entry_path(entry)) %></h2>
        <p><%= entry.date %></p>
        <p><%= entry.contents %></p>
    <% end %>
	  <div>
      <ul>
    <% for url, name in @websites %>
    <li><a href="<%= url %>"><%= name %></a></li>
  <% end %>
</ul>
<%= link_to("New entry", new_entry_path) %>
